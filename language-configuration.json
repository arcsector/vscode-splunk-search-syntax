{
    "comments": {
        "lineComment": "`comment(\" \")`",
        "blockComment": ["```\n", "\n```"],
    },
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"],
        ["`", "`"]
    ],
    "surroundingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"],
        ["`", "`"]
    ],
    "indentationRules": {
        "increaseIndentPattern": "^[^\\|]",
        "decreaseIndentPattern": ""
    }
}
